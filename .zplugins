mattmc3/zephyr path:plugins/utility

# omz
ohmyzsh/ohmyzsh path:plugins/magic-enter
ohmyzsh/ohmyzsh path:plugins/fancy-ctrl-z

# custom
$HOME/miniconda3/etc/profile.d/conda.sh
mattmc3/zsh-safe-rm kind:defer

# my custom plugins
$ZDOTDIR/plugins/xdg
$ZDOTDIR/plugins/abbreviations
$ZDOTDIR/plugins/git
$ZDOTDIR/plugins/otp
$ZDOTDIR/plugins/python

# prompts
romkatv/powerlevel10k kind:fpath
$ZDOTDIR/plugins/prompt

# utilities
romkatv/zsh-bench kind:path
ajeetdsouza/zoxide
# rupa/z
mattmc3/zman

# completions
ohmyzsh/ohmyzsh kind:fpath path:plugins/git
docker/cli kind:fpath path:contrib/completion/zsh
esc/conda-zsh-completion kind:fpath
ohmyzsh/ohmyzsh path:plugins/git
ohmyzsh/ohmyzsh path:plugins/gitfast
bobthecow/git-flow-completion

# final
clarketm/zsh-completions kind:fpath
zsh-users/zsh-completions kind:fpath path:src
mattmc3/zephyr path:plugins/completion
belak/zsh-utils path:completion
mattmc3/zephyr path:plugins/confd

# custom
aloxaf/fzf-tab
freed-wu/fzf-tab-source

# deferred plugins
zdharma-continuum/fast-syntax-highlighting kind:defer
zsh-users/zsh-autosuggestions kind:defer
zsh-users/zsh-history-substring-search kind:defer
amstrad/oh-my-matrix kind:defer

